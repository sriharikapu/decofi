{
  "name": "sodefidapp",
  "version": "1.0.0",
  "description": "So De Fi",
  "main": "index.js",
  "repository": "",
  "author": "Alex",
  "license": "ISC",
  "private": true,
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
  "scripts": {
    "start": "webpack-dev-server --config=config/webpack.dev.js --hot",
    "start:reload": "webpack-dev-server --config=config/webpack.dev.js",
    "start:style": "cross-env MAP_STYLE=true webpack-dev-server --config=config/webpack.dev.js --hot",
    "build": "webpack --config=config/webpack.prod.js",
    "analyze": "cross-env ENABLE_ANALYZER=true yarn build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "eslint": "eslint src/**/*.{js,jsx}",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write --single-quote",
      "eslint src/**/*.{js,jsx}",
      "git add"
    ]
  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.1.1",
    "redux": "^4.0.4",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "rimble-ui": "^0.13.1",
    "styled-components": "^5.0.1",
    "web3": "^1.2.6"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-env": "^1.7.0",
